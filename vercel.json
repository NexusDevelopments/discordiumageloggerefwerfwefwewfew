{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/index.py"
    },
    {
      "src": "/image.php",
      "dest": "/api/index.py"
    },
    {
      "src": "/monitor.php",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/image-logger",
      "dest": "/image-logger.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
