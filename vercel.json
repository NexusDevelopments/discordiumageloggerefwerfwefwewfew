{
  "framework": "vite",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/webhook",
      "destination": "/api/index.py"
    },
    {
      "source": "/image.php",
      "destination": "/api/index.py"
    },
    {
      "source": "/monitor.php",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    }
  ]
}
